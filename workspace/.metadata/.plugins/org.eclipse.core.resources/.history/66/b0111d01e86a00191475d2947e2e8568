public class Deal{
	Home home;
	public double difference;
	
	public Home getHome() {
		return home;
	}

	public void setHome(Home home) {
		this.home = home;
	}
	
	public Deal(Home home, double d){
		this.home = home;
		this.difference = d;
	}
	
	public double getDifference() {
		return difference;
	}
	public void setDifference(double difference) {
		this.difference = difference;
	}
}